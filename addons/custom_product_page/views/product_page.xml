                <div class='Frame_56'>
                </div>
            </div>
        </xpath>
        <xpath expr="//footer" position="replace">

        </xpath>
    </template>
    <template id="custom_homepage" inherit_id="website.homepage" name="Custom Homepage">
        <xpath expr="//t[@t-call='website.layout']" position="replace">
            <t t-call="website.layout">
                <div class="main">
                    <div class='header_content'>
                        <div class='more_than_faster-Div'>
                            <div class='group-1'>
                                <div class='fill'></div>
                                <div class='group-1820550687'>
                                    <img src="/custom_product_page/static/src/img/image_40196.png"
                                        alt="image_40196"
                                        class="image_40196" />
                                    <img src="/custom_product_page/static/src/img/image_40197.png"
                                        alt="image_40197"
                                        class="image_40197" />
                                </div>
                                <p class='more_than_faster'>More than Faster</p>
                            </div>
                            <div class="header_para">
                            <p class="header_para-1">Light, crispy, and nutritious – enjoy</p>
                            <p class="header_para-2">makhana anytime!</p>
                            </div>
                            <p class='header_para-3'>Madhubani Makhana – fresh, crunchy, and nutritious, right at your home!</p>
                        </div>
                    </div>
                </div>
            </t>
        </xpath>
    </template>

