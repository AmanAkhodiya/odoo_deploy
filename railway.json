{
  "$schema": "https://schema.railway.app/railway.schema.json",
  "build": {
      "builder": "NIXPACKS",
      "nixpacksPlan": {
          "providers": ["python"]
      }
  },
  "deploy": {
      "startCommand": "python3 odoo-bin --config=/etc/odoo/odoo.conf",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
  }
}